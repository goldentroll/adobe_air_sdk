<extension xmlns="http://ns.adobe.com/air/extension/3.1">
    <id>com.adeven.adjustio</id>
    <versionNumber>2.2.1</versionNumber>
    <platforms>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>AdjustIo.jar</nativeLibrary>
                <initializer>com.adeven.adjustio.AdjustIoExtension</initializer>
            </applicationDeployment>
        </platform>

        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libAdjustIoExtension.a</nativeLibrary>
                <initializer>AdjustIoExtensionInitializer</initializer>
                <finalizer>AdjustIoExtensionFinalizer</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
